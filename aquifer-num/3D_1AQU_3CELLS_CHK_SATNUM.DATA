RPTRST
 'BASIC=2' /
  

WELSPECS
 'PROD1'  'PROD'   5   12   1*      'OIL'  0.00      'STD'     'SHUT'      'YES'    0      'SEG' /
/

COMPDAT
 'PROD1'   5   12   1   3   'OPEN'   0    6.145  0.216  1*   0.00	 0.0 'Z' 1* /
/

WCONPROD
  'PROD1' OPEN ORAT  1000.0  4* 125.0 /
/  

DATES
 01  'DEC' 2018 /
 01  'JAN' 2019 /
/


