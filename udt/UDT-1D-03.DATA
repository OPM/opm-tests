-- 1D UDT test case for the 'LL' interpolation method.
-- The UDT and UDQs are used to reduce the pressure in two wells as the oil rate drops.

------------------------------------------------------------------------------------------------
RUNSPEC
------------------------------------------------------------------------------------------------

DIMENS
 5 1 4 /

OIL
WATER

METRIC

START
 1 'JAN' 2023 /


EQLDIMS
 1 1*  25 /

REGDIMS
   1          1   /

WELLDIMS
 2        2            10        10  /


TABDIMS
  1          1      100          60         72         60 /

UDTDIMS
  1  10  10 1 /
UDQDIMS
  8*10 /
UDADIMS
  10 1* 100 /

UNIFIN
UNIFOUT

------------------------------------------------------------------------------------------------
GRID
------------------------------------------------------------------------------------------------

NEWTRAN

GRIDFILE
0  1/

TOPS
  5*2000 /
DXV
  5*100 /
DYV
  1*100 /
DZ 
  20*10 /

EQUALS
  PORO 0.25 /
  PERMX 1000 /
  PERMY 1000 /
  PERMZ 200  /
/

INIT

------------------------------------------------------------------------------------------------  
EDIT
------------------------------------------------------------------------------------------------

------------------------------------------------------------------------------------------------
PROPS
------------------------------------------------------------------------------------------------


PVDO
  50  1.1  1.0 
  300 1.0  1.01 /

PVTW
  200 1.0  1.0e-6 /


DENSITY
  900.0 1000. 0.05 /

ROCK
  200.0 1.0e-5 /
 
SWOF
0.1  0  1  0.0   
1.0  1  0  0.0  /

------------------------------------------------------------------------------------------------
REGIONS
------------------------------------------------------------------------------------------------

EQUALS
  EQLNUM 1 /
  SATNUM 1 /
/

------------------------------------------------------------------------------------------------
SOLUTION
------------------------------------------------------------------------------------------------

RPTRST
 BASIC=2 /

EQUIL
2000      200    2100.0   0    1950.000 0.0  / 

SUMMARY
------------------------------------------------------------------------------------------------

ALL
PERFORMA

WU_WBHP 
/
WU_WBHP0
/
------------------------------------------------------------------------------------------------
SCHEDULE
------------------------------------------------------------------------------------------------

TUNING
 0.1 1 /
 /
 20 2 /

WELSPECS 
   'PROD1'   'TEST'   5   1  1*       'OIL'  2*      'STOP'  4* /
   'PROD2'   'TEST'   1   1  1*       'OIL'  2*      'STOP'  4* /
/

COMPDAT 
-- WELL     I	J   K1	K2	  Status    Sat.	CF	  DIAM        KH SKIN ND	DIR    Ro 
   'PROD1'   5	1   1    2    'OPEN'   1*     1* 	  0.216       /
   'PROD2'   1	1   1    1    'OPEN'   1*     1* 	  0.216       /
/


WCONPROD
  'PROD1'  'OPEN'  ORAT   500.0   4*   180.0 /
  'PROD2'  'OPEN'  ORAT   500.0   4*   180.0 /
/

DATES
 2 JAN 2023 /
/
 
-- Reduce BHP limit as pressure drops

UDT
 'TU_FBHP' 1 /
 'LL'  100.0  500.0 / -- FOPR values
       100.0  180.0 / -- WBHP values
/
/

UDQ
ASSIGN WU_WBHP 0 /
DEFINE WU_WBHP0 WU_WBHP /
DEFINE WU_WBHP ((TU_FBHP[WOPR] UMIN WBHP) UMIN WU_WBHP0) UMAX 10.0 /  -- MIN BHP = 10
/

WCONPROD
  'PROD1'  'OPEN'  ORAT   500.0   4*   WU_WBHP /
  'PROD2'  'OPEN'  ORAT   500.0   4*   WU_WBHP /
/
 
DATES
 15 FEB 2023 /
/


END
  
