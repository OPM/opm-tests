RUNSPEC

DIMENS
46 28 22   /

--NOSIM

PARALLEL
  4 /

GRIDOPTS
 'YES' 0 /

OIL

WATER

GAS

DISGAS

VAPOIL

ENDSCALE
NODIR   REVERS /

METRIC

SATOPTS
HYSTER  /

START
 1  'OCT' 2018 /

EQLDIMS
 5 1*  20 /

EQLOPTS
 'THPRES'  /   no fine equilibration if swatinit is being used

REGDIMS
-- ntfip  nmfipr  nrfreg  ntfreg
    22      3      1*      20    /

WELLDIMS
  10  36  10  5 /

TABDIMS
--ntsfun ntpvt nssfun nppvt ntfip nrpvt ntendp
   2     2     33     60   16    60 /


VFPPDIMS
 19  10  10  10  0  3 /

FAULTDIM
10000 /


NSTACK
 30 /

UNIFIN
UNIFOUT

---------------------------------------------------------
--
--	Input of grid geometry
--
---------------------------------------------------------
GRID

NEWTRAN

-- Ask for an EGRID file; no .GRID output.
GRIDFILE
  0  1 /

-- optional for postprocessing of GRID
MAPAXES
 0.  100.  0.  0.  100.  0.  /

-- requests output of INIT file
INIT

MESSAGES
 8*10000  20000 10000 1000 1* /

PINCH
 0.001 GAP  1* TOPBOT TOP/

NOECHO

--------------------------------------------------------
--
--  	Grid and faults
--
--------------------------------------------------------

-- Simulation grid, with slooping faults:
-- file in UTM coordinate system, for importing to DecisionSpace
INCLUDE
 'include/irap_46x28x22.grdecl' / 
--
INCLUDE
 'include/actnum_46x28x22.grdecl' / 


-- Faults
INCLUDE
 'include/faults_46x28x22.faults' / 


-- Alteration of transmiscibility by use of the 'MULTFLT' keyword
INCLUDE
 'include/multflt_46x28x22.inc' / 

--------------------------------------------------------
--
--  	Input of grid parametres
--
--------------------------------------------------------

-- PORO, NTG and PERMX
INCLUDE
 'include/grid_props_46x28x22.grdecl' /

COPY
   PERMX PERMY /
   PERMX PERMZ /
/

-- based on same kv/kh factor
MULTIPLY
   'PERMZ' 0.2    1 46 1  28  1  1 /    Garn 3
   'PERMZ' 0.04   1 46 1  28  2  2 /    Garn 2
   'PERMZ' 0.25   1 46 1  28  3  3 /    Garn 1
   'PERMZ' 0.0    1 46 1  28  4  4 /    Not 	(inactive anyway)
   'PERMZ' 0.13   1 46 1  28  5  5 /    Ile 2.2
   'PERMZ' 0.13   1 46 1  28  6  6 /    Ile 2.1.3
   'PERMZ' 0.13   1 46 1  28  7  7 /    Ile 2.1.2
   'PERMZ' 0.13   1 46 1  28  8  8 /    Ile 2.1.1
   'PERMZ' 0.09   1 46 1  28  9  9 /    Ile 1.3
   'PERMZ' 0.07   1 46 1  28 10 10 /    Ile 1.2
   'PERMZ' 0.19   1 46 1  28 11 11 /    Ile 1.1
   'PERMZ' 0.13   1 46 1  28 12 12 /    Tofte 2.2
   'PERMZ' 0.64   1 46 1  28 13 13 /	Tofte 2.1.3
   'PERMZ' 0.64   1 46 1  28 14 14 /	Tofte 2.1.2
   'PERMZ' 0.64   1 46 1  28 15 15 /	Tofte 2.1.1
   'PERMZ' 0.64   1 46 1  28 16 16 /	Tofte 1.2.2
   'PERMZ' 0.64   1 46 1  28 17 17 /	Tofte 1.2.1
   'PERMZ' 0.016  1 46 1  28 18 18 /	Tofte 1.1
   'PERMZ' 0.004  1 46 1  28 19 19 /	Tilje 4
   'PERMZ' 0.004  1 46 1  28 20 20 /	Tilje 3
   'PERMZ' 1.0    1 46 1  28 21 21 /	Tilje 2
   'PERMZ' 1.0    1 46 1  28 22 22 /	Tilje 1
/


--------------------------------------------------------
--
--      Barriers
--
--------------------------------------------------------

-- Region barriers
INCLUDE
 'include/multz_46x28x22.grdecl' /

-- Field-wide barriers
EQUALS
  'MULTZ'    1.0      1  46  1  28   1   1  / -- Garn3       - Garn 2
  'MULTZ'    0.05     1  46  1  28  15  15  / -- Tofte 2.1.1 - Tofte 1.2.2
  'MULTZ'    0.001    1  46  1  28  18  18  / -- Tofte 1.1   - Tilje 4
  'MULTZ'    0.00001  1  46  1  28  20  20  / -- Tilje 3     - Tilje 2
/

-- Local barriers
INCLUDE
 'include/multz_mod_46x28x22.inc' /
  
-- 20 flux regions generated by the script Xfluxnum
INCLUDE
 './include/fluxnum_46x28x22.inc' /


-- modify transmissibilites between fluxnum using MULTREGT
INCLUDE
 './include/MULTREGT_D_27.prop' / 

NOECHO

MINPV
  500 /

 

EDIT
--------------------------------------------------------------------------------

